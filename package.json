{
  "$schema": "https://fbflipper.com/schemas/plugin-package/v2.json",
  "name": "flipper-plugin-better-network",
  "id": "Network",
  "version": "0.0.2",
  "pluginType": "client",
  "main": "dist/bundle.js",
  "flipperBundlerEntry": "index.tsx",
  "license": "MIT",
  "keywords": [
    "flipper-plugin"
  ],
  "bugs": {
    "url": "https://github.com/bytehala/flipper-plugin-better-network/issues"
  },
  "icon": "apps",
  "title": "Betwork",
  "description": "Betwork - NOT the original Network plugin",
  "scripts": {
    "lint": "flipper-pkg lint",
    "prepack": "flipper-pkg lint && flipper-pkg bundle",
    "build": "flipper-pkg bundle",
    "watch": "flipper-pkg bundle --watch",
    "test": "jest --no-watchman"
  },
  "dependencies": {
    "brotli": "^1.3.3",
    "buffer": "^6.0.3",
    "lodash": "^4.17.21",
    "pako": "^2.0.3",
    "protobufjs": "^6.10.2",
    "xml-beautifier": "^0.4.0"
  },
  "peerDependencies": {
    "@emotion/styled": "latest",
    "antd": "latest",
    "flipper-plugin": "0.203.0",
    "react": "latest",
    "react-dom": "latest"
  },
  "devDependencies": {
    "@babel/preset-react": "latest",
    "@babel/preset-typescript": "latest",
    "@emotion/styled": "latest",
    "@testing-library/react": "latest",
    "@types/brotli": "^1.3.1",
    "@types/jest": "latest",
    "@types/pako": "^1.0.1",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "antd": "latest",
    "flipper-pkg": "latest",
    "jest": "latest",
    "jest-mock-console": "latest",
    "js-base64": "^3.6.0",
    "react": "latest",
    "react-dom": "latest",
    "typescript": "latest"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFiles": [
      "<rootDir>/jest-setup.ts"
    ]
  }
}
